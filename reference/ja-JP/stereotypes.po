#~ "@Target(TYPE)\n"
#~ "@RequestScoped\n"
#~ "@WebTier\n"
#~ "@Stereotype(supportedScopes=RequestScoped.class)\n"
#~ "public @interface Action {}]]>"
#~ msgstr ""
#~ "<![CDATA[@Retention(RUNTIME)\n"
#~ "@Target(TYPE)\n"
#~ "@RequestScoped\n"
#~ "@WebTier\n"
#~ "@Stereotype(supportedScopes=RequestScoped.class)\n"
#~ "public @interface Action {}]]>"

#~ msgid ""
#~ "If a particular action class attempts to specify a scope other than the "
